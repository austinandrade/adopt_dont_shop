<h1>New Application</h1>
<%= form_with url: "/users/", method: :post, local: true do |f| %>
  <%= f.label :full_name %>
  <%= f.text_field :full_name %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zipcode %>
  <%= f.number_field :zipcode %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit :submit %>
<% end %>
